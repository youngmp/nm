# 3:1 reduced forced cgl.
# right-hand sidefunctions are from v2_jupyter/bif2_cgl1_fourier.ipynb
# save 2par to /v2_bifdat_2par/redu/.

p eps=.05,d=0.01
p o1=1,o2=1

h1(x)=2*cos(x*3.0)*(-0.0366)-2*sin(x*3.0)*(-0.0244)
h2(x)=0.1065+2*cos(x*3.0)*(0.5660)-2*sin(x*3.0)*(0.6726)-2*sin(x*6.0)*(-0.0128)

f1(x)=o1*eps*h1(x)
f2(x)=o2*eps^2*h2(x)

x'=-d+f1(x)+f2(x)

@ bounds=10000000
@ dt=.01
@ total=500

@ dsmax=.01,dsmin=.00001,ds=.01,parmax=.3,parmin=-.3